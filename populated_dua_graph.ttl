ns1:City a owl:Class ;
    rdfs:subClassOf ns1:GeographicalLocation .

ns1:Construction a owl:Class ;
    rdfs:subClassOf ns1:Location .

ns1:DivineLocation a owl:Class ;
    rdfs:subClassOf ns1:Location .

ns1:HellLocation a owl:Class ;
    rdfs:subClassOf ns1:AfterlifeLocation .

ns1:HistoricalPlace a owl:Class ;
    rdfs:subClassOf ns1:GeographicalLocation .

ns1:IsPartOf a owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( ns1:Verse :QuranicDua ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( ns1:Chapter ns1:Verse ) ] .

ns1:MentionedIn a owl:ObjectProperty,
        owl:TransitiveProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( ns1:Location ns1:Prophet ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( ns1:Chapter ns1:Verse ) ] .

ns1:Mountain a owl:Class ;
    rdfs:subClassOf ns1:GeographicalLocation .

ns1:ParadiseLocation a owl:Class ;
    rdfs:subClassOf ns1:AfterlifeLocation .

ns1:River a owl:Class ;
    rdfs:subClassOf ns1:GeographicalLocation .

ns1:Sea a owl:Class ;
    rdfs:subClassOf ns1:GeographicalLocation .

ns2:engHadithNo a owl:DatatypeProperty ;
    rdfs:subPropertyOf owl:topDataProperty .

ns2:hadithURL a owl:DatatypeProperty,
        owl:NamedIndividual ;
    rdfs:label "Hadith URL" ;
    rdfs:comment "link to the hadith on the Islamic Urdu Website" .

ns2:hasNarratorSegment a owl:ObjectProperty ;
    rdfs:domain ns2:NarratorChain ;
    rdfs:range ns2:NarratorChainSegment .

ns2:refersToNarrator a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:domain ns2:NarratorChainSegment ;
    rdfs:range ns2:HadithNarrator .

